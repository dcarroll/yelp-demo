<aura:component implements="flexipage:availableForAllPageTypes" controller="ProductController" access="global">

    <aura:attribute name="products" type="Product__c[]"/>

    <aura:handler name="init" value="{!this}" action="{!c.initHandler}" />

    <c:product_tile_list products="{!v.products}"></c:product_tile_list>

</aura:component>	
